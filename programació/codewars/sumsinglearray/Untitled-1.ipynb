    "def repeats(arr):\n",
    "    sum = 0\n",
    "    for num in arr:\n",
    "        if arr.count(num) == 1:\n",
    "            sum = sum + num\n",
    "            #sum += num es lo mateix\n",
    "\n",
    "    return sum\n",
    "        \n",
    "\n",
    "repeats([4, 5, 7, 5, 4, 8,])"
   ]
  }
